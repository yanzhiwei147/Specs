{
  "name": "MUPFoundation",
  "version": "0.3.0",
  "summary": "基于第三方框架以及系统框架的扩展apis",
  "description": "                   MUPFoundation框架是一款基于系统Foundation框架以及部分第三方进行构建的一套增强apis。她主要提供功能是数据库、设备能力封装、系统基础类扩展、网络封装、性能分析工具等.\n",
  "homepage": "http://git.sdp.nd/arida/mupfoundation-ios",
  "license": "MIT",
  "authors": [
    "yanzw"
  ],
  "source": {
    "svn": "http://192.168.9.129/svn/application/65_%E6%A0%A1%E5%8F%8BIOS/01_Trunk/4_DevProcess/3_Src/1_Source/NDChinaDream_Han_SDK/Release/SDK/MUPFoundation"
  },
  "platforms": {
    "ios": 6.0
  },
  "vendored_frameworks " : "MUPFoundation.framework",
  "source_files": "*.framework",
  "public_header_files" : "MUPFoundation.framework/Headers/*.h",
  "preserve_paths" : "*.framework",
  "xcconfig": {
        "FRAMEWORK_SEARCH_PATHS": "$(PODS_ROOT)/MUPFoundation",
        "OTHER_LDFLAGS" : "-framework MUPFoundation"
      },
  "requires_arc": true
}
